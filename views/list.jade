extends layout

block content
  h1= title
  a(href="/", class='btn btn-danger') Regresar
  div 
    p= 'Cupos disponibles: ' + (cupos - count)
    p= 'Vehiculos activos: ' + count
 
  div
    table(class='table')
      thead 
        tr
          th Placa
          th Tipo 
          th Hora entrada
      tbody
        each document in documentos 
          tr 
            td= document.placa 
            if document.tipo
              td Carro 
            else 
              td Moto
            td= document.fecha
        


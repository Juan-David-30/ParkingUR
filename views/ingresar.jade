extends layout

block content
  h1= title
  h2 ingrese un vehiculo: 
  form(action="/entrada", method="post") 
    p placa: 
      input(type="text", name ="placa")  
    p tipo de vehiculo: 
      select(name="tipo")
         option(value="") Elegir 
         option(value="1") Carro 
         option(value="0") Moto   
    button(type="submit", namespace ="Subir", class='btn btn-success') Subir 